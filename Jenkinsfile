pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Develop') {
            steps {
                echo 'Develop code'
            }
        }
        
        stage('Testing') {
            steps {
                echo 'Testing code'
            }
        }
        
         stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
        
    }
}
